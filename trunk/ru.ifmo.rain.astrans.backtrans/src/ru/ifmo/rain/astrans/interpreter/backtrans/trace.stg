group backtrans;

main(transformation) ::= <<


public interface <transformation.traceClassName> {
	<transformation.mappingRules:{rule| <rule:(rule.class.simpleName)()>}; separator="\n\n">
}
>>

ConcreteClassMappingRuleImpl(rule) ::= <<
void <rule.writeTraceStatement.traceMethodName>(<rule.parameter.type> <rule.parameter.name>, <rule.result.type> <rule.result.name>);
>>

AbstractClassMappingRuleImpl(rule) ::= ""